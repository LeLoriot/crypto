{% extends 'base.html.twig' %}

{% block body %}
    <table class="table text-center">
        <tr>
            <th class="active text-center">Nom</th>
            <th class="active text-center">Symbole</th>
            <th class="active text-center">Date de Cr√©ation</th>
            <th class="active text-center">Createur</th>
            <th class="active text-center">Minable</th>
        </tr>

        {% for crypto in cryptos %}
            <tr>
                <td>{{ crypto.nom }}</td>
                <td>{{ crypto.symbole }}</td>
                <td>{{ crypto.dateCreation|date('Y-m-d')}}</td>
                <td>{{ crypto.createur }}</td>
                <td>{{ crypto.minable }}</td>
            </tr>
        {% endfor %}
    </table>
{% endblock %}